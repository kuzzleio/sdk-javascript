{
  "name": "kuzzle-sdk",
  "version": "6.0.0-beta-7",
  "description": "Official Javascript SDK for Kuzzle",
  "author": "The Kuzzle Team <support@kuzzle.io>",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuzzleio/sdk-javascript.git"
  },
  "issues": "https://github.com/kuzzleio/sdk-javascript/issues",
  "keywords": [
    "kuzzle",
    "sdk",
    "baas",
    "backend",
    "realtime",
    "advanced search",
    "bulk"
  ],
  "scripts": {
    "postinstall": "npm run build",
    "prepublish": "npm run build",
    "test": "npm run --silent lint && npm run unit-testing && npm run functional-testing",
    "unit-testing": "nyc --reporter=text-summary --reporter=lcov mocha",
    "functional-testing": "cucumber-js --exit --fail-fast",
    "lint": "eslint --max-warnings=0 ./src ./test",
    "build": "node build.js"
  },
  "browser": "dist/kuzzle.js",
  "main": "index.js",
  "license": "Apache-2.0",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "min-req-promise": "^1.0.1",
    "ora": "^3.1.0",
    "webpack": "^4.29.3",
    "ws": "^6.1.3"
  },
  "devDependencies": {
    "codecov": "^3.2.0",
    "cucumber": "^5.1.0",
    "eslint": "^5.13.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "mocha": "5.2.0",
    "mock-require": "^3.0.3",
    "nyc": "^13.2.0",
    "proxyquire": "^2.1.0",
    "rewire": "^4.0.1",
    "should": "13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^7.2.3"
  },
  "engines": {
    "node": ">= 6.9.1"
  }
}
